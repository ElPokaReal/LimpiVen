{
  "expo": {
    "name": "limpiven",
    "slug": "limpiven",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3411d866-fc8d-4dc1-9fda-11b45c501d84"
      }
    },
    "owner": "jaxor",
    "android": {
      "package": "com.limpiven.limpiven"
    }
  }
}
